    {block:ifNotCaptionsonIndex}{block:PermalinkPage}{block:Caption}
        <div class="caption">{Caption}</div>
    {/block:Caption}{/block:PermalinkPage}{/block:ifNotCaptionsonIndex}
{/block:Link}
   
{block:Chat}
    {block:Title}<div class="t-l chat">
        <a href="{Permalink}" target="_blank">{Title}</a>
    </div>{/block:Title}
    {block:Lines}<div class="line"}>
        {block:Label}<b>{Label}</b>{/block:Label} {Line}
    </div>{/block:Lines}
{/block:Chat}

{block:Audio}
    {block:IfNot400pxPosts}{AudioEmbed-500}{/block:IfNot400pxPosts}
    {block:If400pxPosts}{AudioEmbed-400}{/block:If400pxPosts}
    {block:ifCaptionsonIndex}
        <div class="caption">{Caption}</div>
    {/block:ifCaptionsonIndex}
    {block:ifNotCaptionsonIndex}{block:PermalinkPage}{block:Caption}
        <div class="caption">{Caption}</div>
    {/block:Caption}{/block:PermalinkPage}{/block:ifNotCaptionsonIndex}
{/block:Audio}
 
 
{block:Video}
    {block:IfNot400pxPosts}{Video-500}{/block:IfNot400pxPosts}
    {block:If400pxPosts}{Video-400}{/block:If400pxPosts}
    {block:ifCaptionsonIndex}
        <div class="caption">{Caption}</div>
    {/block:ifCaptionsonIndex}
    {block:ifNotCaptionsonIndex}{block:PermalinkPage}{block:Caption}
        <div class="caption">{Caption}</div>
    {/block:Caption}{/block:PermalinkPage}{/block:ifNotCaptionsonIndex}
{/block:Video}
  
{block:Answer}
    <div class="question">{Question}</div>
    <div class="asker">{Asker}</div>
    <div class="caption">{Replies}</div>
{/block:Answer}
    
        </div><!--- POST --->
    
{block:Date}<div class="info">
    <a href="{Permalink}" title="{TimeAgo}">
        {DayOfMonthWithZero}.{MonthNumberWithZero}.{ShortYear}
    </a>
    {block:NoteCount}&thinsp;&#8729;&thinsp;
        <a href="{Permalink}" title="notes">{NoteCount}</a>
    {/block:NoteCount}
    {block:RebloggedFrom}&emsp;&#8729;&emsp;
    <a href="{ReblogParentURL}" title="{ReblogParentName}" target="_blank">
    via</a>{block:ContentSource}&thinsp;&#8729;&thinsp;
        <a href="{SourceURL}" title="{ReblogRootName}" target="_blank">
        src</a>
    {/block:ContentSource}{/block:RebloggedFrom}
    {block:IndexPage}<div class="right">
        {block:ifReblogLinkonIndex}
            <a href="{ReblogURL}" target="_blank">reblog</a>
        {/block:ifReblogLinkonIndex}
