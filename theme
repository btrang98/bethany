            <!-- {LikeButton} -->
            <div class="like_button"></div>
            <span class="our_button">&hearts;</span>
        </div>{/block:ifLikeButtononIndex}
    </div>{/block:IndexPage}
    
    {block:HasTags}<div class="tags {select:Tags On Index}">
        {block:Tags}<a href="{TagURL}">#{Tag}</a> &nbsp;{/block:Tags}
    </div>{/block:HasTags}
    
</div>{/block:Date}



